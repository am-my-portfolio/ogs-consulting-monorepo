{
  "name": "@am-ogs/react-ui",
  "version": "0.0.1",
  "description": "Library for components that are re-used across all my projects,",
  "author": "afshan.aman@gmail.com",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-ui.umd.js",
  "module": "./dist/react-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-ui.es.js",
      "require": "./dist/react-ui.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/am-my-portfolio/react-components-library.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rimraf ./dist",
    "postinstall": "pnpm build",
    "preview": "vite preview",
    "format": "npx prettier --check .",
    "format:fix": "npx prettier --write .",
    "lint": "eslint \"**/*.js\" \"**/*.ts\" \"**/*.tsx\" \"**/*.jsx\"",
    "lint:fix": "eslint --fix \"**/*.js\" \"**/*.ts\" \"**/*.tsx\" \"**/*.jsx\""
  },
  "dependencies": {},
  "peerDependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@iconify/react": "^5.2.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/react": "^19.1.2",
    "@types/lodash": "^4.17.16",
    "typescript": "^5.8.3",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "@types/node": "^22.14.1",
    "tailwindcss": "^4.1.4",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1",
    "globals": "^16.0.0",
    "eslint": "^9.22.0",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1"
  },
  "keywords": [
    "react",
    "tailwind4"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/am-my-portfolio/react-components-library/issues"
  },
  "homepage": "https://github.com/am-my-portfolio/react-components-library#readme"
}
