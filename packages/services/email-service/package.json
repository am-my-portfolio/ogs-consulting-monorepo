{
  "name": "@am-ogs/email-svc",
  "version": "1.0.0",
  "description": "Email Service",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rimraf node_modules",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "serverless": "serverless deploy --stage local --aws-profile default",
    "serverless:offline": "serverless offline --stage local --aws-profile default",
    "serverless:remove": "serverless remove --stage dev --aws-profile default",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli.js -d src/data-source",
    "typeorm:log": "yarn typeorm:cli schema:log",
    "typeorm:sync": "yarn typeorm:cli schema:sync",
    "typeorm:drop": "yarn typeorm:cli schema:drop",
    "typeorm:reset": "yarn typeorm:drop && yarn typeorm:sync",
    "typeorm:run": "yarn typeorm:cli migration:run",
    "typeorm:show": "yarn typeorm:cli migration:show",
    "typeorm:init": "yarn typeorm:cli migration:generate ./src/migrations/init",
    "typeorm:create": "yarn typeorm:cli migration:create"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@am-ogs/types": "1.0.0",
    "@nestjs/apollo": "^11.0.4",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.7",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.7",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/platform-express": "^9.3.7",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@vendia/serverless-express": "^4.10.1",
    "@sendgrid/client": "8.1.0",
    "@sendgrid/helpers": "^8.0.0",
    "@sendgrid/mail": "8.1.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1314.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.3.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.6.0",
    "moment": "^2.29.4",
    "mongodb": "^5.2.0",
    "neo4j-driver": "^5.7.0",
    "nest-neo4j": "^0.3.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "typeorm": "0.3.11",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.7",
    "@types/aws-lambda": "^8.10.114",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "serverless-jetpack": "0.11.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.2.3",
    "ts-morph": "^18.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
